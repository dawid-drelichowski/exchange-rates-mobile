{
  "parser": "babel-eslint",
  "plugins": [
    "flowtype",
    "react",
    "react-native"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:flowtype/recommended",
    "plugin:react/recommended",
    "plugin:react-native/all"
  ],
  "rules": {
    "semi": ["error", "never"],
    "object-curly-spacing": ["error", "never"],
    "react/prop-types": "off"
  }
}